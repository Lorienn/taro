{
  "name": "taro-docs",
  "private": true,
  "scripts": {
    "start": "NODE_OPTIONS=--max-old-space-size=4096 BASE=taro docusaurus start",
    "build:github": "NODE_OPTIONS=--max-old-space-size=5120 BASE=taro docusaurus build --out-dir build/taro",
    "build": "npm run build:github",
    "build:independent": "NODE_OPTIONS=--max-old-space-size=5120 docusaurus build --out-dir dist",
    "build:zone": "NODE_OPTIONS=--max-old-space-size=5120 BASE=zone docusaurus build --out-dir dist",
    "build:docs-api": "ts-node --project ./scripts/tsconfig.json ./scripts/docs --verbose",
    "swizzle": "docusaurus swizzle",
    "deploy": "docusaurus deploy",
    "serve": "docusaurus serve",
    "start:en": "NODE_OPTIONS=--max-old-space-size=5120 BASE=taro docusaurus start --locale en",
    "write-translations:en": "docusaurus write-translations --locale en"
  },
  "dependencies": {
    "@docusaurus/core": "^2.0.0-beta.8",
    "@docusaurus/plugin-client-redirects": "^2.0.0-beta.8",
    "@docusaurus/plugin-ideal-image": "^2.0.0-beta.8",
    "@docusaurus/plugin-pwa": "^2.0.0-beta.8",
    "@docusaurus/preset-classic": "^2.0.0-beta.8",
    "@docusaurus/remark-plugin-npm2yarn": "^2.0.0-beta.8",
    "@docusaurus/theme-live-codeblock": "^2.0.0-beta.8",
    "clsx": "^1.1.1",
    "color": "^3.1.2",
    "npm-to-yarn": "^1.0.0-2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5",
    "workbox-routing": "^5.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
